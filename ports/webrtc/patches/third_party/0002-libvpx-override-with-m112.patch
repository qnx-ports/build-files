-THREADFN EncoderThreadAdapter(void* obj) {
-  VP9M2MEncoder* p = reinterpret_cast<VP9M2MEncoder*>(obj);
-  p->RunLoopEncode();
-  return THREAD_EXIT_SUCCESS;
-}
